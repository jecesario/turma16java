programa
{
	
	funcao inicio()
	{
		real celsius
		real fahrenheit

		escreva("Digite o valor da temperatura em Celsius: ")
		leia(celsius)

		fahrenheit = (celsius * 9 / 5) + 32

		escreva("Temperatura em Fahrenheit: " + fahrenheit + "°F")

	}
}
